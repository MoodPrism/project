pipeline {
	agent {docker {image 'maven:3.3.3'}}
	
    stages {
        stage('Build') {
	    steps {
               sh "mvn -f moodprism/pom.xml clean package"
            }
        }
        stage('Deploy') {
            steps {
                sh 'mvn -f moodprism/pom.xml deploy -s moodprism/settings.xml'

            }
        }
        stage('Test') {
            steps {
	       sh "Tests........."
            }
        }
    }
}
