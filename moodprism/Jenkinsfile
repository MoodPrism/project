pipeline {
	agent any
	
    stages {
        stage('Build') {
	    steps {
               sh "mvn -f moodprism/pom.xml clean test"
            }
        }
        stage('Test') {
            steps {
                sh "mvn -f  moodprism/pom.xml test"
            }
        }
        stage('Package') {
            steps {
                sh "mvn -f  moodprism/pom.xml package"
            }
        }
    }
}
